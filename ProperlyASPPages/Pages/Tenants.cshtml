@page
@model ProperlyASPPages.Pages.TenantsModel
@{
    ViewData["Title"] = "Tenants";
}

<div class="container">
    <h1 class="display-4">Tenants</h1>
    <p class="lead">Manage tenants and their rental agreements.</p>
    
    <div class="row mt-4">
        <div class="col-md-12">
            <div class="card">
                <div class="card-header">
                    <h3>Tenant Overview</h3>
                </div>
                <div class="card-body">
                    <p>View and manage tenants, lease information, and rental payments.</p>
                    <button class="btn btn-primary">Add New Tenant</button>
                </div>
            </div>
        </div>
    </div>
</div>
